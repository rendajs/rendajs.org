{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "$preact"
	},
	"imports": {
		"$preact": "https://esm.sh/preact@10.13.2",
		"$preact/": "https://esm.sh/preact@10.13.2/",
		"$std/": "https://deno.land/std@0.182.0/"
	},
	"fmt": {
		"options": {
			"indentWidth": 4,
			"useTabs": true,
			"lineWidth": 300,
			"proseWrap": "preserve"
		}
	}
}
